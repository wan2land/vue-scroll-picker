{
  "name": "vue-scroll-picker",
  "version": "0.6.1",
  "description": "Scroll Picker Component for Vue.js.",
  "author": "Changwan Jun",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/wan2land/vue-scroll-picker.git"
  },
  "scripts": {
    "prepack": "npm run build",
    "test": "npm run test:lint",
    "test:lint": "eslint 'src/**/*.ts'",
    "dev:example": "vite serve example",
    "build": "npm run build:production && npm run build:example",
    "build:production": "vite build && tsc -p tsconfig.build.json",
    "build:example": "vite build example",
    "deploy:example": "npm run build:example && push-dir --dir=example-dist --branch=gh-pages"
  },
  "main": "lib/vue-scroll-picker.umd.js",
  "module": "lib/vue-scroll-picker.es.js",
  "types": "lib/index.d.ts",
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@types/node": "^14.14.31",
    "@types/simple-icons": "^4.10.0",
    "@vitejs/plugin-vue": "^1.1.5",
    "@vue/compiler-sfc": "^3.0.6",
    "eslint": "^7.21.0",
    "eslint-config-stable": "^0.9.0",
    "eslint-plugin-vue": "^7.7.0",
    "husky": "^5.1.3",
    "mini-css-extract-plugin": "1.3.9",
    "push-dir": "0.4.1",
    "simple-icons": "^4.13.0",
    "typescript": "^4.2.2",
    "vite": "^2.0.4",
    "vue": "^3.0.6"
  }
}
